    "    def __init__(self, hospitals = None):\n",
    "        if hospitals != None:\n",
    "            self.hospitals = hospitals\n",
    "    \n",
    "    def hospitalize(self, patient: Person):\n",
    "        for hospital in self.hospitals:\n",
    "            if hospital.has_free_places():\n",
    "                hospital.add_patient(patient)\n",
    "                break\n",
    "    \n",
    "    def make_policy(self):\n",
    "        pass\n",
