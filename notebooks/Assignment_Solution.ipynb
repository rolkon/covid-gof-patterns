   "source": [
    "from abc import ABC, abstractmethod\n",
    "\n",
    "class Observer: pass\n",
    "\n",
    "class Observable(ABC):\n",
    "    @abstractmethod\n",
    "    def attach(self, observer: Observer):\n",
    "        pass\n",
    "    \n",
    "    @abstractmethod\n",
    "    def detach(self, observer: Observer):\n",
    "        pass\n",
    "    \n",
    "    @abstractmethod\n",
    "    def notify(self):\n",
    "        pass"
   ]
  },
